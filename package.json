{
  "name": "bun-ssr",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run config/dev.ts",
    "build": "bun run config/build.ts",
    "start": "NODE_ENV=production bun run src/server.ts",
    "lint": "biome lint ./",
    "lint:fix": "biome lint ./ --write",
    "format": "biome format ./ --write",
    "format:check": "biome format ./",
    "type-check": "tsc --noEmit",
    "check": "biome check ./ && bun run type-check"
  },
  "dependencies": {
    "express": "5.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@tailwindcss/cli": "^4.1.17",
    "@types/bun": "1.3.3",
    "@types/express": "5.0.5",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "tailwindcss": "4.1.17",
    "typescript": "5.3.3"
  }
}
